http:
  middlewares:
    secure-headers:
      headers:
        sslRedirect: true
        forceSTSHeader: true
        stsIncludeSubdomains: true
        stsPreload: true
        stsSeconds: 31536000
    auth:
      basicauth:
        users:
          - "${TRAEFIK_AUTH}"

tls:
  options:
    default:
      minVersion: VersionTLS12